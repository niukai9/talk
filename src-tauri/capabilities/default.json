{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:allow-load",
    "sql:default",
    "sql:allow-execute",
    "websocket:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**/"
        },
        {
          "url": "http://**/"
        }
      ]
    },
    "core:window:allow-close",
    "core:window:allow-minimize"
    ]
}